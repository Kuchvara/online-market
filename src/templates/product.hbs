<li class="products-list-item">
  <img src={{image}} alt={{name}} class="product-image"
  onerror="src='https://subscribenow.com.au/time/asia/Solo/Content/Images/noCover.gif'"/>
  <span class="products-item-name">{{name}}</span>
  <span class="products-item-price">{{price}}</span>
  <div class="product-icons">
    <a href="" class="product-icon-box product-link">
      <svg class="product-icon">
        <use href="./images/svg/sprite.svg#icon-search"></use>
      </svg>
    </a>
    <button class="product-icon-box" id="product-cart-btn" data-id="1">
      <svg class="product-icon">
        <use href="./images/svg/sprite.svg#icon-basket"></use>
      </svg>
    </button>
  </div>
</li>